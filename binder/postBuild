#!/bin/bash

# Minimal Binder setup - just install Python packages
echo "Setting up Quantum Networking System for Binder..."

# Create necessary directories
mkdir -p saved_topologies
mkdir -p simulation_logs

# Install only essential Python packages
echo "Installing Python requirements..."
pip install --no-cache-dir fastapi uvicorn pydantic numpy matplotlib ipywidgets requests

echo "Setup complete - backend will start via Procfile"

